<%- include('../partials/head.ejs'); %>
<%- include('../partials/navbar.ejs'); %> 

<div class="showcase-container" data-stellar-background-ratio="0.5" style="background-image: url('/images/one-piece-bg.jpeg')">

  <div class="container">
		<div class="row align-items-center">
			<div class="col-lg">
				<!-- Main Content -->
				<div class="row">
					<div class="col-lg intro">
						<h1 class="text-white font-weight-bold mb-4" data-aos="fade-up" data-aos-delay="0">One Piece Toilet</h1>
					</div>
				</div>
				<div class="row justify-content-center" data-aos="fade-up"> 

					<!--Item start  -->
					<% for(var i=0; i < products.length; i++) { %>
					<div class="col-5 col-md-4 col-lg-3">
						<div class="card mb-4">
							<a href="<%= products[i].imageUrl %>" class="gal-item" data-fancybox="gal"  data-caption="<%= products[i].size %>">
								<img src="<%= products[i].imageUrl %>" alt="Image" class="card-img-top">
							</a>
							<div class="card-body">
								<h5 class="card-title"><%= products[i].name %></h5>
								<p class="card-text"><%= products[i].size %></p>
							</div>
						</div>
					</div>
					<% } %>
					<!--Item End  -->

				</div>
				<!-- Main Content -->
			</div>
		</div>
	</div>
</div>

<%- include('../partials/footer.ejs'); %>
